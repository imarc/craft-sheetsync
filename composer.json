{
    "name": "imarc/craft-sheetsync",
    "description": "Allows you to import CSV, XLSX, and other spreadsheet files into Craft sections as entries.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "csv",
        "xls",
        "xlsx",
        "excel",
        "spreadsheet",
        "import",
        "sync"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Hamer [kh] <kevin@imarc.com>",
            "homepage": "https://www.imarc.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "phpoffice/phpspreadsheet": "^1.3"
    },
    "autoload": {
        "psr-4": {
          "imarc\\sheetsync\\": "src/"
        }
    },
    "extra": {
        "name": "Sheet Sync",
        "handle": "sheet-sync",
        "developer": "Imarc",
        "developerUrl": "https://www.imarc.com",
        "hasCpSettings": false,
        "hasCpSection": false,
        "components": {
            "syncService": "imarc\\sheetsync\\services\\SyncService"
        },
        "class": "imarc\\sheetsync\\Plugin"
    }
}
